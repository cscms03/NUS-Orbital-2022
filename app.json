{
  "expo": {
    "name": "WorkoutEthic",
    "slug": "WorkoutEthic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "packagerOpts": {
        "sourceExts": [
          "cjs"
        ]
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Healthcare application that helps one inside and outside the gym, simplifying the process of working out and motivating the user to persistently exercise. ",
    "githubUrl": "https://github.com/cscms03/NUS-Orbital-2022"
  }
}
